<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TravelRecord.Error">
    <ContentPage.Content>
        <StackLayout Margin="10" HorizontalOptions="Center" VerticalOptions="Center">
            <Label x:Name="ErrorTitle" FontSize="Medium" HorizontalOptions="Center"/>
            <Label x:Name="ErrorMessage" HorizontalOptions="Center"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>